	"item": [
		{
			"name": "Retry",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				},
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"for(let i = 1; i <= 5; i++) {\r",
							"    const postRequest = {\r",
							"        url: 'http://192.168.56.3:8080/api/v1/users',\r",
							"        method: 'GET',\r",
							"    };\r",
							"    setTimeout(function(){\r",
							"        pm.sendRequest(postRequest, (error, response) => {\r",
							"            console.log(i, new Date().getTime());\r",
							"            if(error !== null) {\r",
							"                console.log(error);\r",
							"            }\r",
							"        });\r",
							"    }, 5000*i);\r",
							"}"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
			"request": {
				"method": "GET",
				"header": [],
				"url": "192.168.56.3:8080/api/v1/users"
			},
			"response": []
		},
		{
			"name": "Create user",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Response contains ID 1\", function () {\r",
							"    let responseData = pm.response.json();\r",
							"    pm.expect(1).eql(responseData);\r",
							"});"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
			"request": {
